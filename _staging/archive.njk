metaTitle60Chars: Gareth de Walters' personal website
description: Gareth de Walters' personal website
metaDescription155Chars: Gareth de Walters' personal website
eleventyExcludeFromCollections: true
eleventyNavigation:
  key: Home
  order: 1
eleventyImport:
  collections: ["post"]
numberOfLatestPostsToShow: 6
---

<section class="page-header section-sm">
  <div class="container">
    <div class="row">
      <div class="col-lg-12 text-center">
        <h1 class="section-title h2 mb-3">
          <span>Archive</span>
        </h1>
        <ul class="list-inline breadcrumb-menu mb-3">
          <li class="list-inline-item"><a href="index.html"><i class="ti ti-home"></i>  <span>Home</span></a></li>
          <li class="list-inline-item">• &nbsp; <a href="archive.html"><span>Archive</span></a></li>
        </ul>
      </div>
    </div>
  </div>
</section>




<section>
  <div class="container">
    <div class="row">
      <div class="col-lg-7 mx-auto">
        <div class="archive-block">
            {% for year, posts in collections.postsByYearDescVersion21  %}
              <h2><i class="ti ti-archive"></i>{{ year }}</h2>
              {% for post in posts %}
              <p class="archive-post-item">{{ post.date | readableDateNoYear }}<span>•</span><a href="{{ post.url }}">{{ post.data.title }}</a></p>
              {% endfor %}
            {% endfor %}
        </div>
      </div>
    </div>
  </div>
</section>